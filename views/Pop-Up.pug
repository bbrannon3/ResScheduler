doctype html
html(style='font-size: 16px;')
  head
    meta(content='width=device-width, initial-scale=1.0' name='viewport')
    meta(charset='utf-8')
    meta(content='' name='keywords')
    meta(content='' name='description')
    meta(content='np-template-header-footer-from-plugin' name='page_type')
    title Pop-Up
    link(href='css/nicepage.css' media='screen' rel='stylesheet')
    link(href='css/Pop-Up.css' media='screen' rel='stylesheet')
    script.u-script(defer='' src='js/jquery.js' type='text/javascript')
    script.u-script(defer='' src='js/nicepage.js' type='text/javascript')
    link#u-theme-google-font(href='https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i|Open+Sans:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i' rel='stylesheet')
    link#u-page-google-font(href='https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i' rel='stylesheet')
    meta(content='#478ac9' name='theme-color')
    meta(content='Pop-Up' property='og:title')
    meta(content='' property='og:description')
    meta(content='website' property='og:type')
  body.u-body.u-xl-mode
    section#sec-cf91.u-align-center.u-clearfix.u-custom-color-5.u-section-2(class='pop-up-hidden')
      .u-align-left.u-clearfix.u-sheet.u-sheet-1.u-align-center
        .u-clearfix.u-custom-html.u-expanded-width.u-custom-html-1
          .rendered-form
            div
              h1#control-7621841(class="Time")
                br
            .field-selectname.form-group.formbuilder-select
              label.formbuilder-select-label(for='selectname') Name
              select#selectname(name='selectname' class="Shift-Data")
                for user in Users
                  option#selectname-0(selected='true' value= user["id"])= user["UserName"]
                    
            .field-selectrole.form-group.formbuilder-select
              label.formbuilder-select-label(for='selectrole') Role
              select#selectrole(name='selectrole' class="Shift-Data")
                for role in Roles
                  option#selectrole-0(selected='true' value= role["Id"])= role["Name"]
                  
            .field-workordercount.form-group.formbuilder-text
              label.formbuilder-text-label(for='workordercount')
                | Work Order Count
                br
              input#workordercount(name='workordercount' type='text' class="Shift-Data")
              input(name='Hour_Id' type='hidden' id="hours_shift" class="Shift-Data")

            .field-button-1650394388973.form-group.formbuilder-button
              button#button-1650394388973.btn.btn-danger(name='button-1650394388973' type='button' onclick='closePop()')
                | Cancel
            .field-button-1650394408659.form-group.formbuilder-button
              button#button-1650394408659.btn.btn-success(name='button-1650394408659' type='button' onclick='submitShift()')
                | Submit
                br
