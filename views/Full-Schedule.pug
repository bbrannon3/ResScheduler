      .container
        div(style='overflow-x:auto;')
          // Begin Schedule Table Styling
          style(type='text/css').
            .tg  {border-collapse:collapse;border-spacing:0; text-align: center; color:#ffffff;}
            .tg td{border-color:black;border-style:solid;border-width:1px;font-family:'Roboto';font-size:14px;
            overflow:hidden;padding:10px 5px;word-break:normal;}
            .tg th{border-color:black;border-style:solid;border-width:1px;font-family:'Roboto';font-size:14px;
            font-weight:normal;overflow:hidden;padding:10px 5px;word-break:normal;}
            .tg .tg-vr4i{background-color:#7F9976;font-family:'Roboto';text-align:center;vertical-align:top}
            .tg .tg-baqh{text-align:center;vertical-align:top}
            .tg .tg-0lax{text-align:left;vertical-align:top}
          // End Schedule Table Styling
          // Begin Schedule Table
          table.tg
            thead
              tr
                th.tg-vr4i(colspan='25')
                  a(href='#')
                    i.fa-solid.icon-white.fa-caret-left.fa-2xl
                  -var date = DateInfo["Month"] + " " + DateInfo["Day"] + ", " + DateInfo["Year"]
                    strong= "Week of " + date
                  a(href='#')
                    i.fa-solid.icon-white.fa-caret-right.fa-2xl
            tbody
              tr
                td.tg-vr4i Time
                td.tg-vr4i 12 am
                td.tg-vr4i 1 am
                td.tg-vr4i 2 am
                td.tg-vr4i 3 am
                td.tg-vr4i 4 am
                td.tg-vr4i 5 am
                td.tg-vr4i 6 am
                td.tg-vr4i 7 am
                td.tg-vr4i 8 am
                td.tg-vr4i 9 am
                td.tg-vr4i 10 am
                td.tg-vr4i 11 am
                td.tg-vr4i 12 pm
                td.tg-vr4i 1 pm
                td.tg-vr4i 2 pm
                td.tg-vr4i 3 pm
                td.tg-vr4i 4 pm
                td.tg-vr4i 5 pm
                td.tg-vr4i 6 pm
                td.tg-vr4i 7 pm
                td.tg-vr4i 8 pm
                td.tg-vr4i 9 pm
                td.tg-vr4i 10 pm
                td.tg-vr4i 11 pm
            each val, key in {0:"Monday",1:"Tuesday",2:"Wednesday",3:"Thursday",4:"Friday",5:"Saturday",6:"Sunday"}
              tr
                td.tg-vr4i(rowspan='1')= val 
                - for(x = 0; x < 24; x++)
                  td.tg-0lax.blackText(style={background: "#"+info[key][x]["ShiftRoleColor"]} id= info[key][x]["HourId"] onclick="transferShiftData(\'"+ info[key][x]["HourId"] +"\', \'"+x+"\', \'"+val+"\')")= info[key][x]["WorkerName"]
                     //button.btn(type='button' data-toggle='modal' data-target='#shiftpopup')
                      form(action="/PlaceWorker" method="POST" style={height: "100%", width: "100%"})
                      input(type="hidden" value= info[key][x]["HourId"] name="Hour_Id")
                      input(type="hidden" value= user["userId"] name="User_Id")
                      div(onclick="this.parentNode.submit()" style={height: "25px", width: "25px", cursor: "pointer"})


